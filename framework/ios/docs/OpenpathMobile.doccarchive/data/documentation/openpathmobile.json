e":{"role":"symbol","title":"OpenpathMobileAccessCore","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OpenpathMobileAccessCore"}],"abstract":[{"type":"text","text":"The entry point for all functionality in the OpenpathMobile SDK."}],"identifier":"doc:\/\/org.cocoapods.OpenpathMobile\/documentation\/OpenpathMobile\/OpenpathMobileAccessCore","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"OpenpathMobileAccessCore"}],"url":"\/documentation\/openpathmobile\/openpathmobileaccesscore"},"doc://org.cocoapods.OpenpathMobile/documentation/OpenpathMobile/Release-0.4.4":{"role":"article","title":"iOS SDK Release v0.4.4","abstract":[],"identifier":"doc:\/\/org.cocoapods.OpenpathMobile\/documentation\/OpenpathMobile\/Release-0.4.4","kind":"article","type":"topic","url":"\/documentation\/openpathmobile\/release-0.4.4"},"doc://org.cocoapods.OpenpathMobile/documentation/OpenpathMobile/Release-0.2.1":{"role":"article","title":"Release v0.2.1","abstract":[],"identifier":"doc:\/\/org.cocoapods.OpenpathMobile\/documentation\/OpenpathMobile\/Release-0.2.1","kind":"article","type":"topic","url":"\/documentation\/openpathmobile\/release-0.2.1"},"select-swift.png":{"alt":"Select Swift in two places","type":"image","identifier":"select-swift.png","variants":[{"url":"\/images\/select-swift.png","traits":["1x","light"]}]},"doc://org.cocoapods.OpenpathMobile/documentation/OpenpathMobile/NullCodable":{"role":"symbol","title":"NullCodable","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"NullCodable"}],"abstract":[],"identifier":"doc:\/\/org.cocoapods.OpenpathMobile\/documentation\/OpenpathMobile\/NullCodable","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"NullCodable"}],"url":"\/documentation\/openpathmobile\/nullcodable"},"doc://org.cocoapods.OpenpathMobile/documentation/OpenpathMobile/ProvisionResult":{"role":"symbol","title":"ProvisionResult","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"ProvisionResult"}],"abstract":[{"type":"text","text":"Represents the result of a call to "},{"type":"codeVoice","code":"provision"},{"type":"text","text":"."}],"identifier":"doc:\/\/org.cocoapods.OpenpathMobile\/documentation\/OpenpathMobile\/ProvisionResult","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"ProvisionResult"}],"url":"\/documentation\/openpathmobile\/provisionresult"},"doc://org.cocoapods.OpenpathMobile/documentation/OpenpathMobile/Release-0.1.1":{"role":"article","title":"Release v0.1.1","abstract":[],"identifier":"doc:\/\/org.cocoapods.OpenpathMobile\/documentation\/OpenpathMobile\/Release-0.1.1","kind":"article","type":"topic","url":"\/documentation\/openpathmobile\/release-0.1.1"},"doc://org.cocoapods.OpenpathMobile/documentation/OpenpathMobile/OpenpathTermsOfUseStatus":{"role":"symbol","title":"OpenpathTermsOfUseStatus","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"OpenpathTermsOfUseStatus"}],"abstract":[],"identifier":"doc:\/\/org.cocoapods.OpenpathMobile\/documentation\/OpenpathMobile\/OpenpathTermsOfUseStatus","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"OpenpathTermsOfUseStatus"}],"url":"\/documentation\/openpathmobile\/openpathtermsofusestatus"},"doc://org.cocoapods.OpenpathMobile/documentation/OpenpathMobile/AuthorizationStatusType":{"role":"symbol","title":"AuthorizationStatusType","fragments":[{"kind":"keyword","text":"enum"},{"kind":"text","text":" "},{"kind":"identifier","text":"AuthorizationStatusType"}],"abstract":[],"identifier":"doc:\/\/org.cocoapods.OpenpathMobile\/documentation\/OpenpathMobile\/AuthorizationStatusType","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"AuthorizationStatusType"}],"url":"\/documentation\/openpathmobile\/authorizationstatustype"},"doc